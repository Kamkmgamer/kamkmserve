# When adding additional environment variables, the schema in "/src/env.js"
# should be updated accordingly.

# Recommended for most uses
DATABASE_URL=DATABASE_URL

# Neon Auth environment variables for Next.js
NEXT_PUBLIC_STACK_PROJECT_ID=NEXT_PUBLIC_STACK_PROJECT_ID
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY
STACK_SECRET_SERVER_KEY=STACK_SECRET_SERVER_KEY


CLERK_SECRET_KEY=CLERK_SECRET_KEY

# Optional: enable Basic Auth just for /admin and /api/admin
# If provided, middleware will allow HTTP Basic Auth for admin routes
# and challenge with a browser prompt when Clerk is not signed in.
ADMIN_BASIC_USER=admin
ADMIN_BASIC_PASS=change_me